.tree-node-group
  margin-left 10px
  padding 0px
  .tree-node-item
    padding 0px
    position relative
    &:before
      position absolute
      left -15px
      top 0px
      content ''
      display block
      height 1em
      width 10px
    &:after
      position absolute
      left -15px
      content ''
      display block
      height 100%
    &:last-child
      &:after
        display none
